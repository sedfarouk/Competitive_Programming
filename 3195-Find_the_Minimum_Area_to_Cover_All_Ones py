class Solution:
    def minimumArea(self, grid: List[List[int]]) -> int:
        n, m = len(grid), len(grid[0])

        x1 = y1 = float("inf")
        x2 = y2 = float("-inf")

        for i in range(n):
            for j in range(m):
                if grid[i][j]:
                    x1 = min(i, x1); y1 = min(j, y1)
                    x2 = max(i, x2); y2 = max(j, y2)

        return (x2 - x1 + 1) * (y2 - y1 + 1)

                    
